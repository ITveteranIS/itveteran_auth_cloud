            <version>0.0.1-SNAPSHOT</version>
        </dependency>

        <dependency>
            <groupId>com.plus</groupId>
            <artifactId>common_mybatis_plus</artifactId>
            <version>0.0.1-SNAPSHOT</version>
        </dependency>

        <dependency>
            <groupId>com.redis</groupId>
            <artifactId>common_redis</artifactId>
            <version>0.0.1-SNAPSHOT</version>
        </dependency>


